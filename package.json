    "@nestjs/platform-express": "8.2.6",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/typeorm": "^8.0.3",
    "bcryptjs": "^2.4.3",
    "cache-manager": "3.6.0",
    "cache-manager-redis-store": "2.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "dotenv": "^15.0.0",
    "jsonwebtoken": "^8.5.1",
    "redis": "4.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "7.4.0",
    "sqlite3": "^5.0.2",
